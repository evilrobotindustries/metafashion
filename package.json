{
  "devDependencies": {
    "bulma": "^0.9.3",
    "bulma-timeline": "^3.0.5",
    "@creativebulma/bulma-collapsible": "^1.0.4",
    "esbuild": "^0.14.29",
    "replace-in-file": "^6.3.2"
  },
  "scripts": {
    "build:bulmaCollapsible": "esbuild ./node_modules/@creativebulma/bulma-collapsible --bundle --minify --format=esm --main-fields=browser,module,main --outfile=./assets/bulma-collapsible.min.js",
    "fix:bulmaCollapsible": "replace-in-file 'this._collapsed||(this.element.style.height=this._originalHeight)' '' ./assets/bulma-collapsible.min.js"
  }
}
