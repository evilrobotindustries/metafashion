{
  "browser": {
    "path": "path-browserify",
    "stream": "stream-browserify"
  },
  "dependencies": {
    "@creativebulma/bulma-collapsible": "^1.0.4",
    "bulma": "^0.9.3",
    "bulma-timeline": "^3.0.5",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@internetarchive/snowpack-files-hash": "^1.0.1",
    "esbuild": "^0.14.27",
    "ipfs-core": "^0.14.1",
    "orbit-db": "^0.28.3",
    "snowpack": "^3.8.8"
  },
  "scripts": {
    "build:ipfs": "esbuild ./node_modules/ipfs-core --bundle --minify --format=esm --main-fields=browser,module,main --inject:./node-globals.js --define:process.env.NODE_ENV='\"production\"' --outfile=./assets/ipfs-core.js",
    "build:orbitdb": "esbuild ./node_modules/orbit-db --bundle --minify --format=esm --main-fields=browser,module,main --inject:./node-globals.js --define:process.env.NODE_ENV='\"production\"' --outfile=./assets/orbit-db.js",
    "start": "snowpack dev --secure",
    "build": "snowpack build"
  }
}
